#boostrap enque korte niser code use koro
function theme_enqueue_bootstrap() {

    /* Bootstrap CSS */
    wp_enqueue_style(
        'bootstrap-css',
        'https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css',
        array(),
        '5.3.2'
    );
}
add_action('wp_enqueue_scripts', 'theme_enqueue_bootstrap');
