//

define('DA_BASE_NAME', plugin_basename(__FILE__));
add_filter( 'plugin_action_links_' . DA_BASE_NAME, 'add_action_links' );

function add_action_links($links){
    $list_page = admin_url('edit.php?post_type=book');
    $link = '<a href="'.$list_page.'">settings</a>';
    
    array_push($links, $link);
    return $links;
}

